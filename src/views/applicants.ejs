<div class="container">
        <table class="table table-striped highlight responsive-table striped">
          <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">Name</th>
              <th scope="col">Email</th>
              <th scope="col">Contact</th>
              <th scope="col">Resume</th>
            </tr>
          </thead>
          <tbody>
            <% if(allApplicants) {%>
            <%allApplicants.forEach((applicant)=> {%>
            <tr>
              <th scope="row"><%=applicant.applicantId%></th>
              <td><%=applicant.applicantName%></td>
              <td><%=applicant.applicantEmail%></td>
              <td><%=applicant.applicantPhone%></td>
              <td>
                <a
                  class="btn btn-sm btn-primary"
                  href="http://localhost:3400/resumes/<%=applicant.applicantResume%>"
                  target="_blank"
                  >view</a
                >
              </td>
            </tr>
            <%})%>
            <%}%>
            
          </tbody>
        </table>
      </div>